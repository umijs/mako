{
  "name": "@utoo/pack-cli",
  "description": "Utoo pack cli",
  "version": "0.0.0",
  "author": "xusd320",
  "bin": {
    "utoo-pack": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^4",
    "@utoo/pack": "*"
  },
  "devDependencies": {
    "@types/node": "^18",
    "oclif": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "utoo-pack",
    "dirname": "utoo-pack",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "topics": {
      "build": {
        "description": "Build with production"
      },
      "dev": {
        "description": "Build with development and HMR"
      }
    }
  },
  "scripts": {
    "build": " rm -rf dist && tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme"
  },
  "types": "dist/index.d.ts"
}
