{
  "minify": false,
  "externals": {
    "hoo": "hoo",
    "empty": "",
    "antd": {
      "root": "antd",
      "subpath": {
        "exclude": ["style"],
        "rules": [
          {
            "regex": "/(version|message|notification)$",
            "target": "$1"
          },
          {
            "regex": "/locale/.+$",
            "target": "$EMPTY"
          },
          {
            "regex": "^(?:es|lib)/([a-z-]+)$",
            "target": "$1",
            "targetConverter": "PascalCase"
          },
          {
            "regex": "^(?:es|lib)/([a-z-]+)/([A-Z][a-zA-Z-]+)$",
            "target": "$1.$2",
            "targetConverter": "PascalCase"
          }
        ]
      }
    },
    "script": {
      "root": "ScriptType",
      "script": "https://example.com/lib/script.js"
    }
  }
}
