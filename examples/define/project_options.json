{
  "$schema": "../../packages/pack/config_schema.json",
  "rootPath": "../../",
  "projectPath": "./",
  "config": {
    "define": {
      "AAA": "aaa",
      "BBB": "bbb"
    },
    "entry": [
      {
        "import": "./src/index.ts"
      }
    ],
    "output": {
      "path": "./dist",
      "filename": "[name].[contenthash:6].js",
      "chunkFilename": "[name].[contenthash:8].js",
      "clean": true
    },
    "sourceMaps": false,
    "optimization": {
      "minify": false
    }
  } 
}