{
  "rootPath": "../../",
  "projectPath": "./",
  "config": {
    "entry": [
      {
        "import": "./index.js"       
      }
    ],
    "output": {
      "path": "./dist",
      "filename": "[name].[contenthash:6].js",
      "chunkFilename": "[name].[contenthash:8].js",
      "clean": true
    },
    "optimization": {
      "moduleIds": "named",
      "minify": false
    },
    "externals": {
      "foo": "foo",
      "foo_require": "commonjs foo",
      "foo_require2": {
        "root": "foo",
        "type": "commonjs"
      },
      "foo_import": "esm foo",
      "foo_import2": {
        "root": "foo",
        "type": "esm"
      }
    }
  }
}
