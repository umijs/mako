{
  "rootPath": "../../",
  "projectPath": "./",
  "config": {
    "entry": [
      {
        "import": "./src/index.tsx"
      }
    ],
    "output": {
      "path": "./dist",
      "filename": "[name].[contenthash:8].js",
      "chunkFilename": "[name].[contenthash:8].js",
      "clean": true
    },
    "optimization": {
      "splitChunks": {
        "js": {
          "minChunkSize": 50000,
          "maxChunkCountPerGroup": 40,
          "maxMergeChunkSize": 200000
        }
      }
    },
    "stats": true
  }
}
