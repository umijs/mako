{
  "$schema": "../../packages/pack/config_schema.json",
  "rootPath": "../../",
  "projectPath": "./",
  "config": {
    "entry": [
      {
        "name": "library",
        "import": "./src/index.ts",
        "library": {
          "name": "MyLibrary",
          "export": ["default", "value"]
        }
      }
    ],
    "output": {
      "path": "./dist",
      "filename": "[name].[contenthash:8].js",
      "clean": true
    },
    "optimization": {
      "moduleIds": "named",
      "minify": false
    },
    "externals": {
      "lodash": "$"
    }
  }
}
